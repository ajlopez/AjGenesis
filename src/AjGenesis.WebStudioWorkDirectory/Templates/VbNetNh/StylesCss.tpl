
BODY {
	BACKGROUND-COLOR: white
}

.TopHeader
{
	background-color: midnightblue;
}

.SiteTitle
{
	font-weight: bold;
	font-size: 20px;
	color: #cc3366;
	font-family: Verdana Bold, Helvetica, sans-serif;
}

.TabsBg {
	BORDER-LEFT-COLOR: black; BORDER-BOTTOM-COLOR: black; BORDER-TOP-COLOR: black; BACKGROUND-COLOR: dimgray; BORDER-RIGHT-COLOR: black
}

.Tabs {
	FONT-WEIGHT: bold; FONT-SIZE: 13px; COLOR: gainsboro; FONT-FAMILY: Verdana, Helvetica, sans-serif; TEXT-DECORATION: none;
	border-width:1px;
	border-style:solid;
	height:25px;
}

.SiteLink {
	FONT-WEIGHT: bold; FONT-SIZE: 11px; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
A.SiteLink:link {
	COLOR: white; TEXT-DECORATION: none
}
A.SiteLink:visited {
	COLOR: #eeeeee; TEXT-DECORATION: none
}
A.SiteLink:active {
	COLOR: #eeeeee; TEXT-DECORATION: none
}
A.SiteLink:hover {
	COLOR: white; TEXT-DECORATION: underline
}

.Head
{
	font-weight: 900;
	font-size: 18px;
	color: #cc3366;
	font-family: Verdana, Arial;
	text-decoration: none;
}
.SubHead
{
	font-weight: 900;
	font-size: 14px;
	color: #cc3366;
	font-family: Verdana, Arial;
	text-decoration: none;
}

.SubSubHead {
	FONT-WEIGHT: bold; FONT-SIZE: 11px; COLOR: black; FONT-FAMILY: Verdana, Helvetica, sans-serif
}

.Normal {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; LINE-HEIGHT: 12px; FONT-FAMILY: Verdana, Helvetica, sans-serif
}

P {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; LINE-HEIGHT: 12px; FONT-FAMILY: Verdana, Helvetica, sans-serif
}

.Error {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; LINE-HEIGHT: 12px; FONT-FAMILY: Verdana, Helvetica, sans-serif; color:Red;
}

.Legend {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; LINE-HEIGHT: 12px; FONT-FAMILY: Verdana, Helvetica, sans-serif; color:#666644;
	text-align: left;
}

.Data {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; LINE-HEIGHT: 12px; FONT-FAMILY: Verdana, Helvetica, sans-serif
}

.Input {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; LINE-HEIGHT: 12px; FONT-FAMILY: Verdana, Helvetica, sans-serif
}

.DataTable
{
	border-right: 1px;
	border-top: 1px;
	font-weight: normal;
	font-size: 11px;
	vertical-align: super;
	border-left: 1px;
	line-height: 12px;
	border-bottom: 1px;
	font-family: Verdana, Helvetica, sans-serif;
	border-collapse: collapse;
}

H1
{
	font-weight: normal;
	font-size: 20px;
	color: #ff6600;
	font-family: Verdana, Helvetica, sans-serif;
}
H2 {
	FONT-WEIGHT: normal; FONT-SIZE: 20px; COLOR: #666644; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
H3 {
	FONT-WEIGHT: normal; FONT-SIZE: 12px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
H4 {
	FONT-WEIGHT: normal; FONT-SIZE: 12px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
H5 {
	FONT-WEIGHT: bold; FONT-SIZE: 11px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
DT {
	FONT-WEIGHT: bold; FONT-SIZE: 11px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
H6 {
	FONT-WEIGHT: bold; FONT-SIZE: 11px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
TFOOT {
	FONT-SIZE: 12px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
THEAD {
	FONT-SIZE: 12px; COLOR: darkred; FONT-FAMILY: Verdana, Helvetica, sans-serif
}
TH
{
	font-weight: bold;
	font-size: 12px;
	vertical-align: baseline;
	color: crimson;
	font-family: Verdana, Helvetica, sans-serif;
}

A
{
	font-size: 11px;
	color: crimson;
	font-family: Verdana, Arial;
	text-decoration: none;
}

