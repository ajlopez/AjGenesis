		<Property>
			<Name>Address</Name>
			<Type>Text</Type>
			<SqlType>text</SqlType>
		</Property>

		<Property>
			<Name>Notes</Name>
			<Type>Text</Type>
			<SqlType>text</SqlType>
		</Property>

	</Properties>
</Entity>

